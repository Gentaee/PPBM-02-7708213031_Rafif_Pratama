<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:layout_width="450dp"
        android:layout_height="70dp"
        android:background="@drawable/shape2"
        android:id="@+id/kotakbiru">
    </ImageView>
    <ImageView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="55dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/shape3"
        android:layout_centerHorizontal="true">
    </ImageView>
    <ImageView
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/seacrh"
        android:layout_centerHorizontal="true"
        android:paddingRight="150dp"
        android:id="@+id/search">
    </ImageView>



    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/search"
        android:layout_marginTop="53dp">

        <ImageView
            android:id="@+id/train"
            android:layout_width="79dp"
            android:layout_height="match_parent"
            android:paddingLeft="20dp"
            android:src="@drawable/hgl"
            android:layout_marginLeft="30dp">

        </ImageView>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="80dp"
            android:layout_marginLeft="-20dp"
            android:src="@drawable/airplane"
            android:id="@+id/order1">
            >

        </ImageView>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:src="@drawable/car"
            android:layout_marginLeft="45dp">

        </ImageView>




    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="various parts"
        android:layout_marginTop="140dp"
        android:paddingLeft="25dp"
        android:fontFamily="@font/poppinsbold"
        android:layout_marginLeft="10dp"
        android:textSize="15sp">

    </TextView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="order"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="-80dp"
        android:paddingLeft="250dp"
        android:fontFamily="@font/poppinsbold"
        android:textSize="15sp">

    </TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Towing"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="-40dp"
        android:paddingLeft="345dp"
        android:fontFamily="@font/poppinsbold"
        android:textSize="15sp">

    </TextView>

    <View
        android:layout_width="match_parent"
        android:visibility="visible"
        android:layout_height="6dp"
        android:layout_marginTop="270dp"
        android:background="@color/biru"
        android:id="@+id/garis">

    </View>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:hint="Search"
        android:layout_marginTop="69dp"
        android:paddingRight="60dp"
        android:fontFamily="@font/poppinsregular"
        android:textSize="15dp"
        android:background="@color/SecondaryColor">

    </EditText>



    <ImageView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_shape"
        android:backgroundTint="@color/abuterang">

    </ImageView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Model"
        android:layout_marginTop="305dp"
        android:layout_marginLeft="27dp"
        android:fontFamily="@font/poppinsbold"
        android:textColor="@color/SecondaryColor"
        android:id="@+id/proms">

    </TextView>
    <ImageView
        android:layout_width="110dp"
        android:layout_height="30dp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="150dp"
        android:background="@drawable/button_shape">

    </ImageView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="     Fitment!"
        android:layout_marginTop="305dp"
        android:layout_marginLeft="158dp"
        android:fontFamily="@font/poppinsbold"
        android:textColor="@color/SecondaryColor"
        android:id="@+id/textrekomen">

    </TextView>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="300dp"
        android:background="@drawable/button_shape"
        android:backgroundTint="@color/abuterang">

    </ImageView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Attention!"
        android:layout_marginTop="305dp"
        android:layout_marginLeft="320dp"
        android:fontFamily="@font/poppinsbold"
        android:textColor="@color/SecondaryColor"
        android:id="@+id/bokung">

    </TextView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Welcome User matic!"
        android:fontFamily="@font/poppinsextra"
        android:gravity="center"
        android:textColor="@color/SecondaryColor"
        android:layout_marginTop="15dp"
        android:textSize="20sp"
        android:paddingRight="50dp">

    </TextView>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="65dp"
        android:src="@drawable/vavarioan"
        android:layout_marginLeft="320dp">

    </ImageView>



    <TextView
        android:id="@+id/keluargaberencana"
        android:layout_width="400dp"
        android:layout_height="215dp"
        android:layout_below="@id/textrekomen"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/poppinsbold"
        android:paddingLeft="25dp"
        android:text="what is fitment? "
        android:textSize="30dp">

    </TextView>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/textrekomen"
        android:layout_marginTop="35dp"
        android:paddingLeft="11dp"
        android:text=" - Menurunkan ukuran standar shock "
        android:fontFamily="@font/poppinsbold"
        android:textSize="15dp"
        android:id="@+id/diskon">

    </TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="11dp"
        android:text=" - mengganti ukuran standar ban "
        android:fontFamily="@font/poppinsbold"
        android:textSize="15dp"
        android:id="@+id/gratis"
        android:layout_below="@id/textrekomen"
        android:layout_marginTop="54dp">

    </TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:paddingLeft="11dp"
        android:text=" - memperganteng penampilan  "
        android:fontFamily="@font/poppinsbold"
        android:textSize="15dp"
        android:id="@+id/gratis2"
        android:layout_marginTop="73dp"
        android:layout_below="@id/textrekomen">

    </TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:paddingLeft="11dp"
        android:text="   "
        android:fontFamily="@font/poppinsbold"
        android:textSize="15dp"
        android:id="@+id/gratis3"
        android:layout_marginTop="92dp"
        android:layout_below="@id/textrekomen">

    </TextView>

    <Button
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:background="@drawable/button_shape"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/textrekomen"
        android:layout_marginTop="120dp"
        android:text="Check"
        android:fontFamily="@font/poppinsbold"
        android:textColor="@color/SecondaryColor"
        android:id="@+id/buttomsheet"
        >

    </Button>





</RelativeLayout>